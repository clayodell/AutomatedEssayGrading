Sub Splitter()
'
' Splitter Macro
' Macro recorded 12/17/2006 by Neel Patel
'
    Selection.Copy
    Documents.Add Template:="Normal", NewTemplate:=False, DocumentType:=0
    Selection.PasteAndFormat (wdPasteDefault)
    ChangeFileOpenDirectory "C:\Documents and Settings\Neel Regular\Desktop\"
    ActiveDocument.SaveAs FileName:="Split.doc", FileFormat:=wdFormatDocument, _
         LockComments:=False, Password:="", AddToRecentFiles:=True, WritePassword _
        :="", ReadOnlyRecommended:=False, EmbedTrueTypeFonts:=False, _
        SaveNativePictureFormat:=False, SaveFormsData:=False, SaveAsAOCELetter:= _
        False
    ActiveWindow.Close
    Selection.Cut
End Sub

Sub Macro2()
'
' Macro2 Macro
' Macro recorded 11/24/2006 by Neel Patel
'
' Keyboard Shortcut: Ctrl+e
' Calculates vocabulary statistics

    Sheets("Data Collection Sheet").Select
    Range("B16:B4849").Select
    Selection.ClearContents
    Range("B16").Select
    ActiveSheet.Paste
    Range("J28").Select
    ActiveSheet.PivotTables("PivotTable1").PivotCache.Refresh
    ActiveWindow.SmallScroll Down:=-1
    Range("A2:R2").Select
    Sheets("Data Recording Sheet").Select
    Range("A2:R2").Select
    Selection.EntireRow.Insert
    Sheets("Data Collection Sheet").Select
    Range("A2:R2").Select
    Selection.Copy
    Sheets("Data Recording Sheet").Select
    Selection.PasteSpecial Paste:=xlPasteValues, 
        Operation:=xlNone, SkipBlanks:=False, Transpose:=False
    Selection.Font.Bold = False
    Sheets("Data Collection Sheet").Select
    Range("J28").Select
    ActiveSheet.PivotTables("PivotTable1").PivotCache.Refresh
    ActiveWorkbook.Save
End Sub

